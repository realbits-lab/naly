      "path": "src/blocks/contact-02/contact-02.tsx",
      "content": "import { Button } from \"@/components/ui/button\";\nimport { Card, CardContent } from \"@/components/ui/card\";\nimport { Checkbox } from \"@/components/ui/checkbox\";\nimport { Input } from \"@/components/ui/input\";\nimport { Label } from \"@/components/ui/label\";\nimport { Textarea } from \"@/components/ui/textarea\";\nimport { MailIcon, MapPinIcon, MessageCircle, PhoneIcon } from \"lucide-react\";\nimport Link from \"next/link\";\n\nconst Contact02Page = () => (\n  <div className=\"min-h-screen flex items-center justify-center py-16\">\n    <div className=\"w-full max-w-screen-xl mx-auto px-6 xl:px-0\">\n      <b className=\"text-muted-foreground\">Contact Us</b>\n      <h2 className=\"mt-3 text-3xl md:text-4xl font-bold tracking-tight\">\n        Chat to our friendly team\n      </h2>\n      <p className=\"mt-3 text-base sm:text-lg\">\n        We&apos;d love to hear from you. Please fill out this form or shoot us\n        an email.\n      </p>\n      <div className=\"mt-24 grid lg:grid-cols-2 gap-16 md:gap-10\">\n        <div className=\"grid grid-cols-1 sm:grid-cols-2 gap-x-8 gap-y-12\">\n          <div>\n            <div className=\"h-12 w-12 flex items-center justify-center bg-primary/10 text-primary rounded-full\">\n              <MailIcon />\n            </div>\n            <h3 className=\"mt-6 font-semibold text-xl\">Email</h3>\n            <p className=\"my-2.5 text-muted-foreground\">\n              Our friendly team is here to help.\n            </p>\n            <Link\n              className=\"font-medium text-primary\"\n              href=\"mailto:akashmoradiya3444@gmail.com\"\n            >\n              akashmoradiya3444@gmail.com\n            </Link>\n          </div>\n          <div>\n            <div className=\"h-12 w-12 flex items-center justify-center bg-primary/10 text-primary rounded-full\">\n              <MessageCircle />\n            </div>\n            <h3 className=\"mt-6 font-semibold text-xl\">Live chat</h3>\n            <p className=\"my-2.5 text-muted-foreground\">\n              Our friendly team is here to help.\n            </p>\n            <Link className=\"font-medium text-primary\" href=\"#\">\n              Start new chat\n            </Link>\n          </div>\n          <div>\n            <div className=\"h-12 w-12 flex items-center justify-center bg-primary/10 text-primary rounded-full\">\n              <MapPinIcon />\n            </div>\n            <h3 className=\"mt-6 font-semibold text-xl\">Office</h3>\n            <p className=\"my-2.5 text-muted-foreground\">\n              Come say hello at our office HQ.\n            </p>\n            <Link\n              className=\"font-medium text-primary\"\n              href=\"https://map.google.com\"\n              target=\"_blank\"\n            >\n              100 Smith Street Collingwood <br /> VIC 3066 AU\n            </Link>\n          </div>\n          <div>\n            <div className=\"h-12 w-12 flex items-center justify-center bg-primary/10 text-primary rounded-full\">\n              <PhoneIcon />\n            </div>\n            <h3 className=\"mt-6 font-semibold text-xl\">Phone</h3>\n            <p className=\"my-2.5 text-muted-foreground\">\n              Mon-Fri from 8am to 5pm.\n            </p>\n            <Link\n              className=\"font-medium text-primary\"\n              href=\"tel:akashmoradiya3444@gmail.com\"\n            >\n              +1 (555) 000-0000\n            </Link>\n          </div>\n        </div>\n\n        {/* Form */}\n        <Card className=\"bg-accent shadow-none\">\n          <CardContent className=\"p-6 md:p-10\">\n            <form>\n              <div className=\"grid md:grid-cols-2 gap-x-8 gap-y-5\">\n                <div className=\"col-span-2 sm:col-span-1\">\n                  <Label htmlFor=\"firstName\">First Name</Label>\n                  <Input\n                    placeholder=\"First name\"\n                    id=\"firstName\"\n                    className=\"mt-1.5 bg-white h-11 shadow-none\"\n                  />\n                </div>\n                <div className=\"col-span-2 sm:col-span-1\">\n                  <Label htmlFor=\"lastName\">Last Name</Label>\n                  <Input\n                    placeholder=\"Last name\"\n                    id=\"lastName\"\n                    className=\"mt-1.5 bg-white h-11 shadow-none\"\n                  />\n                </div>\n                <div className=\"col-span-2\">\n                  <Label htmlFor=\"email\">Email</Label>\n                  <Input\n                    type=\"email\"\n                    placeholder=\"Email\"\n                    id=\"email\"\n                    className=\"mt-1.5 bg-white h-11 shadow-none\"\n                  />\n                </div>\n                <div className=\"col-span-2\">\n                  <Label htmlFor=\"message\">Message</Label>\n                  <Textarea\n                    id=\"message\"\n                    placeholder=\"Message\"\n                    className=\"mt-1.5 bg-white shadow-none\"\n                    rows={6}\n                  />\n                </div>\n                <div className=\"col-span-2 flex items-center gap-2\">\n                  <Checkbox id=\"acceptTerms\" />\n                  <Label htmlFor=\"acceptTerms\">\n                    You agree to our{\" \"}\n                    <Link href=\"#\" className=\"underline\">\n                      terms and conditions\n                    </Link>\n                    .\n                  </Label>\n                </div>\n              </div>\n              <Button className=\"mt-6 w-full\" size=\"lg\">\n                Submit\n              </Button>\n            </form>\n          </CardContent>\n        </Card>\n      </div>\n    </div>\n  </div>\n);\n\nexport default Contact02Page;\n",
      "type": "registry:block",
      "target": "components/contact-02/contact-02.tsx"
    }
  ]
}